    "url": "https://github.com/tmcw/leaflet-geojson-stream/issues"
  },
  "dependencies": {
    "hyperquest": "~0.3.0",
    "geojson-stream": "0.0.0",
    "through": "~2.3.4"
  },
  "devDependencies": {
    "browserify": "~3.44.2",
    "express": "~3.4.1",
    "leaflet": "~0.7.2",
    "uglifyjs": "~2.3.6"
  }
